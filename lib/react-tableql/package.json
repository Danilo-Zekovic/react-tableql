{
  "name": "react-tableql",
  "version": "0.2.1",
  "description": "React component for generating tables and data grids",
  "main": "build/index.js",
  "typings": "build/src/index.d.ts",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Danilo-Zekovic/react-tableql"
  },
  "license": "MIT",
  "keywords": [
    "react",
    "javascript",
    "data",
    "graphql",
    "table",
    "grid",
    "react-tableql",
    "apollo",
    "datagrid",
    "client"
  ],
  "author": {
    "name": "Danilo Zekovic",
    "url": "https://github.com/Danilo-Zekovic"
  },
  "scripts": {
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "start": "webpack --watch",
    "build": "webpack"
  },
  "peerDependencies": {
    "@apollo/client": "^3.0.0",
    "graphql": "^15.0.0",
    "react": "^17.0.2"
  },
  "devDependencies": {
    "@apollo/client": "^3.7.1",
    "@babel/core": "^7.20.5",
    "@babel/plugin-proposal-class-properties": "^7.18.6",
    "@babel/plugin-proposal-object-rest-spread": "^7.20.2",
    "@babel/plugin-transform-react-jsx": "^7.19.0",
    "@babel/polyfill": "^7.12.1",
    "@babel/preset-env": "^7.20.2",
    "@babel/preset-react": "^7.18.6",
    "@babel/preset-typescript": "7.13.0",
    "@testing-library/react": "^12.1.5",
    "@testing-library/react-hooks": "^7.0.2",
    "@types/jest": "26.0.24",
    "@types/node": "^16.18.3",
    "@types/react": "^17.0.52",
    "apollo-cache-inmemory": "^1.6.6",
    "babel-jest": "27.0.5",
    "babel-loader": "^8.3.0",
    "core-js": "^3.26.1",
    "css-loader": "^5.2.7",
    "fork-ts-checker-webpack-plugin": "^6.5.2",
    "graphql": "^15.8.0",
    "graphql-tools": "^4.0.8",
    "jest": "^27.5.1",
    "jest-css-modules-transform": "4.3.0",
    "react": "^17.0.2",
    "style-loader": "^3.3.1",
    "ts-jest": "27.0.3",
    "ts-loader": "^9.4.1",
    "typescript": "^4.9.3",
    "typescript-plugin-css-modules": "3.4.0",
    "webpack": "^5.75.0",
    "webpack-cli": "^4.10.0",
    "webpack-node-externals": "^3.0.0"
  },
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "jsdom",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx"
    ],
    "moduleNameMapper": {
      "\\.(css|less|scss)$": "<rootDir>/__mocks__/styleMock.js"
    },
    "testMatch": [
      "**/?(*.)+(test).ts?(x)"
    ],
    "collectCoverageFrom": [
      "src/**/*.{ts,tsx}",
      "!src/**/index.ts"
    ],
    "coverageThreshold": {
      "global": {
        "statements": 90,
        "branches": 90,
        "functions": 90,
        "lines": 90
      }
    }
  }
}
