{
  "name": "tableql",
  "version": "0.2.1",
  "description": "React table component that is integrated with Apollo Client and GraphQL",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Danilo-Zekovic/react-tableql"
  },
  "keywords": [
    "react",
    "table",
    "react-tableql",
    "graphql",
    "apollo",
    "datagrid",
    "client"
  ],
  "bugs": {
    "url": "https://github.com/Danilo-Zekovic/react-tableql/issues"
  },
  "homepage": "https://github.com/Danilo-Zekovic/react-tableql#readme",
  "private": true,
  "workspaces": [
    "examples/**/*",
    "lib/**/*"
  ],
  "scripts": {
    "format": "prettier --write src stories",
    "format:check": "prettier --check examples lib",
    "bootstrap": "lerna bootstrap",
    "bootstrap:clean": "rm -rf yarn.lock && find . -name \"node_modules\" -type d -prune -exec rm -rf '{}' + && lerna clean && lerna bootstrap",
    "build": "lerna run build",
    "test": "lerna run test",
    "graph": "yarn nx graph",
    "lint": "eslint \"{lib,examples}/**/*.tsx\" --fix",
    "dev": "sh dev.sh",
    "devLife": "docker build --tag tableql-dev . && docker run --init --rm -p 3000:3000 -p 4000:4000 tableql-dev"
  },
  "author": {
    "name": "Danilo Zekovic"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^5.45.0",
    "@typescript-eslint/parser": "^5.45.0",
    "eslint": "^8.28.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-react": "^7.31.11",
    "eslint-plugin-react-hooks": "^4.6.0",
    "husky": "^4.3.7",
    "lerna": "^6.0.3",
    "prettier": "^2.8.0",
    "pretty-quick": "^3.1.3",
    "typescript": "^4.9.3"
  },
  "dependencies": {}
}
