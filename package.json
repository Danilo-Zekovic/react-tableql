{
  "name": "tableql",
  "version": "0.2.1",
  "description": "React table component that is integrated with Apollo Client and GraphQL",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Danilo-Zekovic/react-tableql"
  },
  "keywords": [
    "react",
    "table",
    "react-tableql",
    "graphql",
    "apollo",
    "datagrid",
    "client"
  ],
  "bugs": {
    "url": "https://github.com/Danilo-Zekovic/react-tableql/issues"
  },
  "homepage": "https://github.com/Danilo-Zekovic/react-tableql#readme",
  "private": true,
  "workspaces": [
    "examples/**/*",
    "lib/**/*"
  ],
  "scripts": {
    "format": "prettier --write src stories",
    "format:check": "prettier --check examples lib",
    "bootstrap": "lerna bootstrap",
    "bootstrap:clean": "rm -rf yarn.lock && find . -name \"node_modules\" -type d -prune -exec rm -rf '{}' + && lerna clean && lerna bootstrap",
    "build": "lerna run build",
    "test": "lerna run test",
    "graph": "yarn nx graph"
  },
  "author": {
    "name": "Danilo Zekovic"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^4.28.1",
    "@typescript-eslint/parser": "^4.28.1",
    "eslint": "^7.30.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-react": "^7.24.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "husky": "^4.3.7",
    "lerna": "^6.0.3",
    "prettier": "^2.3.2",
    "pretty-quick": "^3.1.1",
    "typescript": "^4.3.5"
  },
  "dependencies": {}
}
