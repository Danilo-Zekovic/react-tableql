{
  "name": "react-tableql",
  "version": "0.2.1",
  "description": "React table component that is integrated with Apollo Client and GraphQL",
  "main": "build/index.js",
  "typings": "build/src/index.d.ts",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Danilo-Zekovic/react-tableql"
  },
  "keywords": [
    "react",
    "table",
    "react-tableql",
    "graphql",
    "apollo",
    "datagrid",
    "client"
  ],
  "bugs": {
    "url": "https://github.com/Danilo-Zekovic/react-tableql/issues"
  },
  "homepage": "https://github.com/Danilo-Zekovic/react-tableql#readme",
  "files": [
    "build/index.js",
    "build/src/**/*"
  ],
  "peerDependencies": {
    "@apollo/client": "^3.0.0",
    "react": "^17.0.0",
    "graphql": "^15.0.0"
  },
  "dependencies": {},
  "scripts": {
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "start": "webpack --watch",
    "build": "webpack",
    "build:all": "webpack && build-storybook -o docs",
    "format": "prettier --write src stories",
    "format:check": "prettier --check src stories",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook -o docs"
  },
  "author": {
    "name": "Danilo Zekovic",
    "email": "danilozeka93@gmail.com"
  },
  "devDependencies": {
    "@apollo/client": "^3.3.11",
    "@babel/core": "^7.12.17",
    "@babel/plugin-proposal-class-properties": "^7.12.13",
    "@babel/plugin-proposal-object-rest-spread": "^7.12.13",
    "@babel/plugin-transform-react-jsx": "^7.12.17",
    "@babel/polyfill": "^7.12.1",
    "@babel/preset-env": "^7.12.17",
    "@babel/preset-react": "^7.12.13",
    "@storybook/addon-a11y": "^6.1.18",
    "@storybook/addon-actions": "^6.1.18",
    "@storybook/addon-console": "^1.2.3",
    "@storybook/addon-links": "^6.1.18",
    "@storybook/addons": "^6.1.18",
    "@storybook/preset-typescript": "^3.0.0",
    "@storybook/react": "^6.1.21",
    "@testing-library/react": "^9.5.0",
    "@types/jest": "^26.0.20",
    "@types/react": "^17.0.2",
    "apollo-cache-inmemory": "^1.6.6",
    "apollo-storybook-react": "^0.2.4",
    "babel-jest": "^26.6.3",
    "babel-loader": "^8.2.1",
    "core-js": "^3.9.0",
    "css-loader": "^5.0.2",
    "fork-ts-checker-webpack-plugin": "^6.1.0",
    "graphql": "^15.5.0",
    "graphql-tools": "^4.0.8",
    "husky": "^4.3.7",
    "jest": "^26.6.3",
    "prettier": "^2.2.0",
    "pretty-quick": "^3.1.0",
    "react": "^17.0.1",
    "storybook-addon-jsx": "^7.3.6",
    "storybook-readme": "^5.0.9",
    "style-loader": "^2.0.0",
    "ts-jest": "^26.5.1",
    "ts-loader": "^8.0.17",
    "typescript": "^4.1.5",
    "webpack": "^4.46.0",
    "webpack-cli": "^4.5.0",
    "webpack-node-externals": "^2.5.2"
  },
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "jsdom",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx"
    ],
    "moduleNameMapper": {
      "\\.(css|less|scss)$": "<rootDir>/__mocks__/styleMock.js"
    },
    "testMatch": [
      "**/?(*.)+(test).ts?(x)"
    ],
    "collectCoverageFrom": [
      "src/**/*.{ts,tsx}",
      "!**/*.stories.*"
    ],
    "coveragePathIgnorePatterns": [
      "src/index.ts"
    ],
    "coverageThreshold": {
      "global": {
        "statements": 90,
        "branches": 90,
        "functions": 90,
        "lines": 90
      }
    }
  }
}
