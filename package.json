{
  "name": "react-tableql",
  "version": "0.1.0-beta.4",
  "description": "React table component that is integrated with Apollo Client and GraphQL",
  "main": "build/index.node.js",
  "browser": "build/index.js",
  "typings": "build/src/index.d.ts",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Danilo-Zekovic/react-tableql"
  },
  "keywords": [
    "react",
    "table",
    "react-tableql",
    "graphql",
    "apollo",
    "datagrid",
    "client"
  ],
  "bugs": {
    "url": "https://github.com/Danilo-Zekovic/react-tableql/issues"
  },
  "homepage": "https://github.com/Danilo-Zekovic/react-tableql#readme",
  "files": [
    "build/**/*"
  ],
  "peerDependencies": {
    "react": ">= 16.8.0 < 17",
    "apollo-boost": ">= 0.1.28",
    "graphql": "^15.0.0",
    "react-apollo": ">= 3 < 4"
  },
  "dependencies": {},
  "scripts": {
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "start": "webpack --watch",
    "build": "webpack",
    "build:all": "webpack && build-storybook -o docs",
    "format": "prettier --write src stories",
    "format:check": "prettier --check src stories",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook -o docs"
  },
  "author": {
    "name": "Danilo Zekovic",
    "email": "danilozeka93@gmail.com"
  },
  "devDependencies": {
    "@apollo/react-testing": "^3.1.4",
    "@babel/core": "^7.11.1",
    "@babel/plugin-proposal-class-properties": "^7.10.4",
    "@babel/plugin-proposal-object-rest-spread": "^7.11.0",
    "@babel/plugin-transform-react-jsx": "^7.10.4",
    "@babel/polyfill": "^7.10.4",
    "@babel/preset-env": "^7.11.0",
    "@babel/preset-react": "^7.10.4",
    "@storybook/addon-a11y": "^5.3.19",
    "@storybook/addon-actions": "^5.3.19",
    "@storybook/addon-console": "^1.2.1",
    "@storybook/addon-links": "^5.3.19",
    "@storybook/addons": "^5.3.19",
    "@storybook/preset-typescript": "^3.0.0",
    "@storybook/react": "^5.3.19",
    "@testing-library/react": "^9.5.0",
    "@types/jest": "^26.0.8",
    "@types/react": "^16.9.44",
    "apollo-boost": "^0.4.9",
    "apollo-storybook-react": "^0.2.4",
    "babel-jest": "^26.2.2",
    "babel-loader": "^8.1.0",
    "css-loader": "^4.2.0",
    "fork-ts-checker-webpack-plugin": "^5.0.14",
    "graphql": "^15.3.0",
    "graphql-tools": "^4.0.8",
    "husky": "^4.2.5",
    "jest": "^26.2.2",
    "prettier": "^2.0.5",
    "pretty-quick": "^2.0.1",
    "react": "^16.13.1",
    "react-apollo": "^3.1.5",
    "storybook-addon-jsx": "^7.3.3",
    "storybook-readme": "^5.0.8",
    "style-loader": "^1.2.1",
    "ts-jest": "^26.1.4",
    "ts-loader": "^8.0.2",
    "typescript": "^3.9.7",
    "webpack": "^4.44.1",
    "webpack-cli": "^3.3.12",
    "webpack-node-externals": "^2.5.0"
  },
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "jsdom",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx"
    ],
    "moduleNameMapper": {
      "\\.(css|less|scss)$": "<rootDir>/__mocks__/styleMock.js"
    },
    "testMatch": [
      "**/?(*.)+(test).ts?(x)"
    ],
    "collectCoverageFrom": [
      "src/**/*.{ts,tsx}",
      "!**/*.stories.*"
    ],
    "coverageThreshold": {
      "global": {
        "statements": 90,
        "branches": 90,
        "functions": 90,
        "lines": 90
      }
    }
  }
}
