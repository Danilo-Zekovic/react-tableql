{
  "name": "react-tableql",
  "version": "0.0.8",
  "description": "React table component that is integrated with Apollo Client and GraphQL",
  "main": "build/index.js",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Danilo-Zekovic/react-tableql"
  },
  "keywords": [
    "react",
    "table",
    "react-tableql",
    "graphql",
    "apollo",
    "datagrid",
    "client"
  ],
  "bugs": {
    "url": "https://github.com/Danilo-Zekovic/react-tableql/issues"
  },
  "homepage": "https://github.com/Danilo-Zekovic/react-tableql#readme",
  "files": [
    "src/**/*",
    "build/**/*",
    "docs/**/*"
  ],
  "peerDependencies": {
    "react": ">= 16.8.0 < 17",
    "react-dom": ">= 16.8.0 < 17",
    "apollo-boost": ">= 0.1.28",
    "graphql": ">= 14 < 15",
    "react-apollo": ">= 3 < 4"
  },
  "dependencies": {},
  "scripts": {
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "start": "webpack --watch",
    "build": "webpack",
    "build:all": "webpack && build-storybook -o docs",
    "format": "prettier --write \"src/**/*.{js, jsx, css, json}\"",
    "format:check": "prettier --list-different \"src/**/*.{js, jsx, css, json}\"",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook -o docs"
  },
  "author": {
    "name": "Danilo Zekovic",
    "email": "danilozeka93@gmail.com"
  },
  "devDependencies": {
    "@apollo/react-testing": "^3.1.3",
    "@babel/core": "^7.7.2",
    "@babel/plugin-proposal-class-properties": "^7.7.0",
    "@babel/plugin-proposal-object-rest-spread": "^7.6.2",
    "@babel/plugin-transform-react-jsx": "^7.7.0",
    "@babel/polyfill": "^7.7.0",
    "@babel/preset-env": "^7.7.1",
    "@babel/preset-react": "^7.7.0",
    "@storybook/addon-a11y": "^5.2.5",
    "@storybook/addon-actions": "^5.2.5",
    "@storybook/addon-console": "^1.2.1",
    "@storybook/addon-links": "^5.2.5",
    "@storybook/addons": "^5.2.5",
    "@storybook/react": "^5.2.5",
    "@testing-library/react": "^9.3.2",
    "apollo-boost": "^0.4.4",
    "apollo-storybook-react": "^0.2.2",
    "babel-jest": "^24.9.0",
    "babel-loader": "^8.0.6",
    "css-loader": "^2.1.1",
    "graphql": "^14.5.8",
    "graphql-tools": "^4.0.6",
    "jest": "^24.9.0",
    "prettier": "^1.18.2",
    "react": "^16.11.0",
    "react-apollo": "^3.1.3",
    "react-dom": "^16.11.0",
    "storybook-addon-jsx": "^7.1.13",
    "storybook-readme": "^5.0.8",
    "style-loader": "^0.23.1",
    "webpack": "^4.41.2",
    "webpack-cli": "^3.3.10"
  },
  "jest": {
    "moduleNameMapper": {
      "\\.(css|less)$": "<rootDir>/__mocks__/styleMock.js"
    },
    "collectCoverageFrom": [
      "src/**/*.js"
    ],
    "coverageThreshold": {
      "global": {
        "statements": 90,
        "branches": 90,
        "functions": 90,
        "lines": 90
      }
    }
  }
}
